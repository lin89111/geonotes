CREATE TABLE notes (ID BIGINT NOT NULL, CREATIONDATE VARCHAR(255), DESCRIPTION VARCHAR(255), IDCREATOR BIGINT, X FLOAT, Y FLOAT, PRIMARY KEY (ID))
CREATE TABLE routes (ID BIGINT NOT NULL, CREATIONDATE VARCHAR(255), IDCREATOR BIGINT, NAME VARCHAR(255), PRIMARY KEY (ID))
CREATE TABLE steps (ID BIGINT NOT NULL, IDNOTE BIGINT, IDROUTE BIGINT, PRIMARY KEY (ID))
CREATE TABLE users (ID BIGINT GENERATED BY DEFAULT AS IDENTITY NOT NULL, LOGIN VARCHAR(255), PASSWORD VARCHAR(255), PRIMARY KEY (ID))
CREATE TABLE SEQUENCE (SEQ_NAME VARCHAR(50) NOT NULL, SEQ_COUNT DECIMAL(15), PRIMARY KEY (SEQ_NAME))
